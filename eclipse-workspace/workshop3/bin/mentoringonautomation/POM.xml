<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.gm</groupId>
	<artifactId>onstar-test</artifactId>
	<version>1.0-SNAPSHOT</version>

	<properties>

		<perfectoUserName>shafkat.ali@gm.com</perfectoUserName>
		<perfectoPassword>freedom1!</perfectoPassword>
		<perfectoDeviceName>1115FBE2E2CC3504</perfectoDeviceName>
		<!--  Set value as iOS/android based on execution needs -->
		<platform>android</platform>
		<!--  
			Example : 
						For Android use the value All_Versions for osversion
						For iOS 10 & below use the value 10x_And_Below_Versions for osversion
						For iOS 11 & above use the value 11x_And_Above_Versions for osversion
		  -->
		<osversion>All_Versions</osversion>
		<application>myGMC</application>
		<testngXmlFile>gm_prod_positive_ice_cucumber.xml</testngXmlFile>
		<testdatafilename>testdata_Sue.xml</testdatafilename>
		<envt>prod</envt>
		<envt.endpoint>idt_5</envt.endpoint>
		<locale>en_US</locale>
		<perfecto.capabilities.app.fixed>PUBLIC:E2ETesting/JenkinsBuild/Android/myGMC.apk</perfecto.capabilities.app.fixed>
		<!--  if there is space in country and region name, we must use underscore instead of space
		 Example : use South_Korea for South Korea   --> 
		<select.user.country>United_States</select.user.country>
		<select.user.region>North_America</select.user.region>
		<!--  If app needs to be installed set value as Yes else set it as No   -->
		<uninstall.all.app>Yes</uninstall.all.app>
		<!--  Set value as Validation/Release/Production based on which build needs to be tested  -->
		<build.type>Validation</build.type>
		<!--  Set value as Cucumber/TestNG based on execution approach -->
		<execution.context>Cucumber</execution.context>
		<testngXmlDir>config</testngXmlDir>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<qaf.version>2.1.11</qaf.version>
		<sourceVersion>1.8</sourceVersion>
		<targetVersion>1.8</targetVersion>
		<reportium-sdk.version>1.1.15</reportium-sdk.version>
		<perfectomobile.version>9.0.0.0</perfectomobile.version>
		<selenium.version>2.53.1</selenium.version>
		<testng.version>6.10</testng.version>
		<cukes.version>1.2.5</cukes.version>
		<org.json.version>20160810</org.json.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
	</properties>

	<build>
		<resources>
			<resource>
				<directory>src/test/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>

		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<compilerVersion>${sourceVersion}</compilerVersion>
					<source>${sourceVersion}</source>
					<target>${targetVersion}</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>aspectj-maven-plugin</artifactId>
				<version>1.7</version>
				<executions>
					<execution>
						<id>compile</id>
						<goals>
							<goal>compile</goal>
						</goals>
					</execution>
					<execution>
						<id>test-compile</id>
						<goals>
							<goal>test-compile</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<source>${sourceVersion}</source>
					<target>${targetVersion}</target>
					<showWeaveInfo>true</showWeaveInfo>
					<complianceLevel>${sourceVersion}</complianceLevel>
					<aspectLibraries>
						<aspectLibrary>
							<groupId>com.qmetry</groupId>
							<artifactId>qaf</artifactId>
						</aspectLibrary>
					</aspectLibraries>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.9</version>
				<configuration>
					<argLine>
						-Dfile.encoding=${project.build.sourceEncoding}
						-Dplatform=${platform}
						-Dosversion=${osversion}
						-Dapplication=${application}
						-Dlocale=${locale}
						-Dperfecto.capabilities.user=${perfectoUserName}
						-Dperfecto.capabilities.password=${perfectoPassword}
						-Dperfecto.capabilities.deviceName=${perfectoDeviceName}
						-Denvt=${envt}
						-Denvt.endpoint=${envt.endpoint}
						-Dtestdatafilename=${testdatafilename}
						-Dperfecto.capabilities.app.fixed=${perfecto.capabilities.app.fixed}
						-Dselect.user.country=${select.user.country}
						-Dselect.user.region=${select.user.region}
						-Duninstall.all.app=${uninstall.all.app}
						-Dexecution.context=${execution.context}
						-Dbuild.type=${build.type}
						-Dhttp.proxyHost=10.122.173.14
						-Dhttp.proxyPort=80
						-Dhttps.proxyHost=10.122.173.14
						-Dhttps.proxyPort=443
						-Dhttp.nonProxyHosts=*.gm.com
						-Dhttp.proxyUser=
						-Dhttp.proxyPassword=
						-Dhttps.proxyUser=
						-Dhttps.proxyPassword=					
					</argLine>
					<suiteXmlFiles>
						<suiteXmlFile>${testngXmlDir}/${testngXmlFile}</suiteXmlFile>
					</suiteXmlFiles>
					<systemPropertyVariables>
						<org.uncommons.reportng.xml-dialect>testng</org.uncommons.reportng.xml-dialect>
						<org.uncommons.reportng.escape-output>false</org.uncommons.reportng.escape-output>
						<application.properties.file>resources/application.properties</application.properties.file>
					</systemPropertyVariables>
				</configuration>
			</plugin>
		</plugins>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>org.codehaus.mojo</groupId>
										<artifactId>aspectj-maven-plugin</artifactId>
										<versionRange>[1.0,)</versionRange>
										<goals>
											<goal>test-compile</goal>
											<goal>compile</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<execute />
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<repositories>
		<repository>
			<id>qaf</id>
			<url>https://qmetry.github.io/qaf/dist</url>
		</repository>
		<repository>
			<releases>
				<updatePolicy>always</updatePolicy>
				<checksumPolicy>ignore</checksumPolicy>
			</releases>
			<id>ps</id>
			<url>https://github.com/Project-Quantum/mvn/raw/master/repository</url>
		</repository>
		<repository>
			<id>perfectomobile</id>
			<name>Perfecto mobile</name>
			<url>http://repository-perfectomobile.forge.cloudbees.com/public</url>
		</repository>
		<repository>
			<id>jai</id>
			<url>https://repository.jboss.org/nexus/content/repositories/thirdparty-releases</url>
		</repository>
	</repositories>

	<dependencies>
		<dependency>
			<groupId>org.hamcrest</groupId>
			<artifactId>hamcrest-core</artifactId>
			<version>1.3</version>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.17</version>
		</dependency>
		<dependency>
			<groupId>xml-apis</groupId>
			<artifactId>xml-apis</artifactId>
			<version>1.4.01</version>
		</dependency>
		<dependency>
			<groupId>info.cukes</groupId>
			<artifactId>cucumber-java</artifactId>
			<version>${cukes.version}</version>
		</dependency>
		<dependency>
			<groupId>io.appium</groupId>
			<artifactId>java-client</artifactId>
			<version>4.1.2</version>
		</dependency>
		<dependency>
			<groupId>org.json</groupId>
			<artifactId>json</artifactId>
			<version>${org.json.version}</version>
		</dependency>
		<dependency>
			<groupId>com.qmetry</groupId>
			<artifactId>qaf</artifactId>
			<version>${qaf.version}</version>
