package casting;

public class BadCasting {

	public static void main(String[] args) {
		
		//when super type refecerence to sub type object, classcatexception will occur
//		Person p1 = new Person();
//		Engineer e1 = (Engineer) p1;
		Object o = new Object();
		String s = (String) o;
		
		Person p2 = new Engineer("Ali", 33, 22344);
		Engineer e2 = (Engineer) p2;
	}

}
